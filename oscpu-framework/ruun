./build.sh -e cpu_axi_diff -d -s -a "-i non-output/cpu-tests/matrix-mul-cpu-tests.bin --dump-wave -b 0" -m "EMU_TRACE=1 WITH_DRAMSIM3=1" -b
cd projects/cpu_axi_diff/build/
